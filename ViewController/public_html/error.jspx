<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <f:view>
    <af:document id="d1" title="Scripless | Error Page">
      <af:form id="f1" defaultCommand="cb1">
        <af:panelStretchLayout id="psl1"
                               inlineStyle='background-image:url("images/wp-aurora-blue.png");'
                               topHeight="121px">
          <f:facet name="center">
            <af:panelGroupLayout id="pgl1" layout="vertical" halign="center"
                                 styleClass="AFStretchWidth">
              <af:image source="/images/logo-bmri-med-white.png"
                        shortDesc="Bank Mandiri" id="i1"/>
              <af:spacer width="10" height="50" id="s1"/>
              <af:outputText value="Oops! The application has encountered an unknown error."
                             id="ot1"
                             inlineStyle="color:White; font-size:small; font-weight:bold;"/>
              <af:spacer width="10" height="10" id="s2"/>
              <af:outputText value="It doesn't appear to have affected your data, but our technical staff have been automatically notified and will be looking into this with the utmost urgency."
                             id="ot2"
                             inlineStyle="color:White; font-size:small;"/>
              <af:spacer width="10" height="40" id="s3"/>
              <af:commandButton text="Back to Login Page" id="cb1"
                                action="login" inlineStyle="padding:5px;"/>
            </af:panelGroupLayout>
          </f:facet>
          <f:facet name="top">
            <af:spacer width="10" height="50" id="s4"/>
          </f:facet>
        </af:panelStretchLayout>
      </af:form>
    </af:document>
  </f:view>
</jsp:root>
